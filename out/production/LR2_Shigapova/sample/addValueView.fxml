<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.AddValueController">
  <children>
    <Pane fx:id="dialogPane" prefHeight="95.0" prefWidth="196.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <children>
        <TextField fx:id="inputTextField" layoutX="8.0" layoutY="30.0" prefWidth="113.0" />
        <ComboBox fx:id="positionComboBox" layoutX="130.0" layoutY="30.0"/>
        <Label layoutX="8.0" layoutY="14.0" text="Введите значение:" />
        <Button fx:id="applyBtn" onAction="#applyClick" layoutX="8.0" layoutY="61.0" mnemonicParsing="false" prefWidth="88.0" text="OK" />
        <Button fx:id="cancelBtn" onAction="#cancelClick" layoutX="104.0" layoutY="62.0" mnemonicParsing="false" prefWidth="79.0" text="Отмена" />
      </children>
    </Pane>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
